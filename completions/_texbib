#compdef texbib
#
# this is zsh completion function file.
# generated by genzshcomp(ver: 0.5.2)
#

comands=(
    "add\:'Add the content of files to the active bibliography'"
    "rm\:'Remove a reference from the active bibliography'"
    "dump\:'Create a bibtex file with all references in active bibliography'"
    "create\:'Create a new bibliography'"
    "delete\:'Delete a bibliography'"
    "open\:'Activate a bibliography'"
    "list\:'List all available bibliographies'"
    "show\:'List the content of the active bibliography'"
    "find\:'Seach in local bibliographies'"
    "search\:'Seach in the web'"
    "gc\:'Clean up'"
)

_texbib() {
    typeset -A opt_args
    local context state line

    _arguments -s -S \
      "-h[show this help message and exit]:" \
      "--help[show this help message and exit]:" \
      "--gen-zsh-comp" \
      "-z" \
      "*::args:(($comands))"
}
